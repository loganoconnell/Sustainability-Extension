{
  "manifest_version": 2,
  "name": "Sustainability Extension",
  "version": "1.0",
  "content_scripts": [
      {
          "matches": [
              "https://www.amazon.com/adidas-Originals-Superstar-Running-Medium/*"
          ],
          "js": ["jquery-3.4.1.min.js", "content.js"]
      }
  ],
  "browser_action": {
      "default_icon": "icon-red.png"
  },
  "background": {
      "scripts": ["background.js"]
  }
}
